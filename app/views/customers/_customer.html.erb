<div id="<%= dom_id customer %>">
  <p>
    <strong>First name:</strong>
    <%= customer.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= customer.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= customer.email %>
  </p>

  <p>
    <strong>Visits:</strong>
    <%= customer.visits %>
  </p>

  <p>
    <strong>Orders count:</strong>
    <%= customer.orders_count %>
  </p>

  <p>
    <strong>Lock version:</strong>
    <%= customer.lock_version %>
  </p>

</div>
