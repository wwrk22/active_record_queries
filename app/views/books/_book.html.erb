<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Year published:</strong>
    <%= book.year_published %>
  </p>

  <p>
    <strong>Isbn:</strong>
    <%= book.isbn %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= book.price %>
  </p>

  <p>
    <strong>Out of print:</strong>
    <%= book.out_of_print %>
  </p>

  <p>
    <strong>Views:</strong>
    <%= book.views %>
  </p>

  <p>
    <strong>Supplier:</strong>
    <%= book.supplier_id %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author_id %>
  </p>

</div>
